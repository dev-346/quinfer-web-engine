    "concurrently": "^8.2.2",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "eslint": "latest",
    "eslint-config-next": "^15.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "5.7.2",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.quinfer.desktop",
    "productName": "Quinfer",
    "files": [
      "main.js",
      "preload.js",
      "package.json",
      ".next/standalone/**/*"
    ],
    "asarUnpack": [
      ".next/standalone/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "createDesktopShortcut": true,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  }
}
